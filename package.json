{
  "license": "MIT",
  "name": "react-baseline-inputs",
  "version": "0.0.0",
  "repository": "https://github.com/promptworks/react-baseline-inputs",
  "description": "React components for creating data-driven forms.",
  "main": "dist/index.js",
  "module": "dist/react-baseline-inputs.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "yarn --cwd example run -s start",
    "build": "tsdx build",
    "test": "TZ=America/New_York tsdx test",
    "lint": "tsdx lint src test",
    "prepare": "tsdx build"
  },
  "prettier": {
    "arrowParens": "avoid",
