      "src/**/*.{ts,tsx}"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTests.ts"
    ]
  },
  "peerDependencies": {
    "react": ">=16.8"
  },
  "dependencies": {
    "fast-deep-equal": "^3.1.3"
  },
  "devDependencies": {
