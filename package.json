    "@testing-library/react": "^10.0.1",
    "@testing-library/react-hooks": "^3.3.0",
    "@types/jest-axe": "^3.2.1",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "babel-jest": "^26.1.0",
    "jest": "^26.1.0",
    "jest-axe": "^3.4.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-test-renderer": "^16.13.1",
